<?xml version="1.0" encoding="utf-8"?>
<shape xmlns:android="http://schemas.android.com/apk/res/android"
    android:shape="rectangle">
    <!-- Màu nền xanh teal -->
    <solid android:color="#00838F"/> <!-- Hoặc màu teal bạn đang dùng, ví dụ #0097A7 -->
    <!-- Viền trắng -->
    <stroke
        android:width="1dp"
        android:color="@android:color/white" />
    <!-- Bo góc giống nút trên -->
    <corners android:radius="25dp"/> <!-- Điều chỉnh radius nếu cần -->
</shape>